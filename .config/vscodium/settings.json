{
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "chat.commandCenter.enabled": false,
  "continue.enableTabAutocomplete": true,
  "continue.pauseTabAutocompleteOnBattery": true,
  "continue.telemetryEnabled": false,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.renderSideBySide": false,
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 7,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Lilex Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.minimap.enabled": false,
  "editor.minimap.showSlider": "always",
  "editor.occurrencesHighlight": "off",
  "editor.padding.bottom": 10,
  "editor.padding.top": 10,
  "editor.quickSuggestionsDelay": 200,
  "editor.renderLineHighlight": "all",
  "editor.rulers": [
    50,
    72,
    80
  ],
  "editor.scrollbar.horizontal": "visible",
  "editor.scrollbar.horizontalScrollbarSize": 2,
  "editor.scrollbar.verticalScrollbarSize": 10,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "explorer.confirmDelete": false,
  "explorer.excludeGitIgnore": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "files.associations": {
    "*.lpl": "json",
    "*.opt": "properties",
    "*.rmp": "properties",
    "*.slangp": "properties",
    "config": "properties"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.openRepositoryInParentFolders": "never",
  "github.gitProtocol": "ssh",
  "markdown.preview.fontSize": 15,
  "partialDiff.enableTelemetry": false,
  "python.analysis.nodeExecutable": "/opt/homebrew/bin/node",
  "redhat.telemetry.enabled": false,
  "security.promptForLocalFileProtocolHandling": false,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "Ghostty.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.fontSize": 13,
  "window.autoDetectColorScheme": true,
  "window.closeWhenEmpty": true,
  "window.customTitleBarVisibility": "windowed",
  "window.density.editorTabHeight": "compact",
  "window.openWithoutArgumentsInNewWindow": "on",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ff0000",
    "editorIndentGuide.activeBackground1": "#ff000030",
    "editorIndentGuide.background1": "#00000001",
    "editorRuler.foreground": "#88888811",
    "scrollbarSlider.background": "#00000028",
    "terminalCursor.foreground": "#ff0000"
  },
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.showTabs": "single",
  "workbench.editor.tabActionLocation": "left",
  "workbench.enableExperiments": false,
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "Mono Atom",
  "workbench.preferredLightColorTheme": "Github Light Theme",
  "workbench.startupEditor": "newUntitledFile",
  "xml.server.workDir": "~/.cache/lemminx"
}
