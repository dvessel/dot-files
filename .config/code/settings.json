{
  "editor.fontFamily": "AnonymicePro Nerd Font Propo",
  "editor.fontSize": 15,
  "editor.wordWrapColumn": 120,
  "github.gitProtocol": "ssh",
  "redhat.telemetry.enabled": false,
  "terminal.integrated.cursorStyle": "line",
  "window.autoDetectColorScheme": true,
  "window.openFilesInNewWindow": "on",
  "diffEditor.experimental.showMoves": true,
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 7,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "editor.minimap.showSlider": "always",
  "editor.occurrencesHighlight": "off",
  "editor.padding.bottom": 10,
  "editor.padding.top": 10,
  "editor.quickSuggestionsDelay": 200,
  "editor.renderLineHighlight": "all",
  "editor.rulers": [72, 80, 120],
  "editor.scrollbar.horizontal": "visible",
  "editor.scrollbar.horizontalScrollbarSize": 4,
  "editor.scrollbar.verticalScrollbarSize": 10,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.allowedCharacters": {
    "‚ùØ": true
  },
  "explorer.excludeGitIgnore": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autorefresh": false,
  "markdown.preview.fontSize": 15,
  "intelephense.telemetry.enabled": false,
  "partialDiff.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.lineHeight": 1.2,
  "window.closeWhenEmpty": true,
  "window.newWindowDimensions": "inherit",
  "window.openWithoutArgumentsInNewWindow": "on",
  "window.restoreFullscreen": true,
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ff0000",
    "editorIndentGuide.activeBackground1": "#ff000030",
    "editorIndentGuide.background1": "#00000001",
    "editorRuler.foreground": "#66666628",
    "scrollbarSlider.background": "#00000028"
  },
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabActionLocation": "left",
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "eq-material-theme-icons",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "One Dark Pro Darker",
  "workbench.preferredLightColorTheme": "Github Light Theme",
  "workbench.startupEditor": "newUntitledFile",
  "xml.server.workDir": "~/.cache/lemminx",
  "editor.minimap.enabled": false
}
