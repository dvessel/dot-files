# Ignore base directory files and folders.
/[a-zA-Z0-9]*

# Dot files not to track.
.cache
.CFUserTextEncoding
.colima
.conda
.config/bandcamp-dl.json
.config/gh
.config/nvim/lazy-lock.json
.config/zed
.cups
.ddev
.diffusionbee
.docker
.ipython
.jupyter
.lesshst
.lima
# Inverse .local selection. !entries tracks changes.
.local/**
!.local/*/
!.local/bin/_*
!.local/bin/bdefaults
!.local/bin/fbrew
# CrossOver .mailcap, .mime.types
.mailcap
.mime.types
.mutagen
.ngrok2
.npm
.python_history
.sqlite_history
.ssh
.swiftpm
.tldrc
.Trash
.vim
.viminfo
.vscode
.zsh_history
.zsh_plugins.zsh
.zsh_sessions
# Don't track local files.
.zshrc-source/*.local.zsh
